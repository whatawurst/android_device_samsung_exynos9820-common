# genfs_contexts

# PROC
genfscon proc /sys/vm/swappiness                           u:object_r:proc_swapiness:s0

# SYSFS
# bus
genfscon sysfs /bus/iio/devices                            u:object_r:sysfs_iio:s0

# class
genfscon sysfs /class/input/input4                         u:object_r:sysfs_sensor_writable:s0
genfscon sysfs /class/input/input6                         u:object_r:sysfs_sensor_writable:s0
genfscon sysfs /class/power_supply                         u:object_r:sysfs_battery:s0
genfscon sysfs /class/sec                                  u:object_r:sysfs_sec:s0
genfscon sysfs /class/sensors                              u:object_r:sysfs_sensor_writable:s0

# devices
genfscon sysfs /devices/platform/10970000.spi/             u:object_r:sysfs_spi_writeable:s0
genfscon sysfs /devices/platform/13d00000.dwmmc2/mmc_host  u:object_r:sysfs_mmc_host:s0
genfscon sysfs /devices/platform/13d60000.ufs/host0        u:object_r:sysfs_scsi_host:s0
genfscon sysfs /devices/platform/15c40000.adc/iio          u:object_r:sysfs_iio:s0
genfscon sysfs /devices/platform/15d60000.hsi2c/i2c-13/13-0066/max77705-charger/power_supply/max77705-charger/type    u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/15d60000.hsi2c/i2c-13/13-0066/max77705-charger/power_supply/otg/type    u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/15d60000.hsi2c/i2c-13/13-0066/max77705-fuelgauge/power_supply/max77705-fuelgauge/type    u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/15d70000.hsi2c/i2c-14/14-003b/power_supply/mfc-charger/type    u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/19030000.decon_f          u:object_r:sysfs_decon:s0
genfscon sysfs /devices/platform/19030000.decon_f/vsync    u:object_r:sysfs_ss_writable:s0
genfscon sysfs /devices/platform/19050000.decon_t          u:object_r:sysfs_decon:s0
genfscon sysfs /devices/platform/19050000.decon_t/vsync    u:object_r:sysfs_ss_writable:s0
genfscon sysfs /devices/platform/19060000.decon_s          u:object_r:sysfs_decon:s0
genfscon sysfs /devices/platform/19060000.decon_s/vsync    u:object_r:sysfs_ss_writable:s0
genfscon sysfs /devices/platform/battery/power_supply      u:object_r:sysfs_battery:s0
genfscon sysfs /devices/platform/npu@17800000/version      u:object_r:sysfs_npu_version:s0
genfscon sysfs /devices/platform/panel@0/lcd/panel         u:object_r:sysfs_lcd_writable:s0
genfscon sysfs /devices/soc0/soc_id                        u:object_r:sysfs_socinfo:s0
genfscon sysfs /devices/system/chip-id                     u:object_r:sysfs_ss_writable:s0

genfscon sysfs /devices/virtual/lcd/panel                  u:object_r:sysfs_lcd_writable:s0
genfscon sysfs /devices/virtual/sec                        u:object_r:sysfs_sec:s0
genfscon sysfs /devices/virtual/sensors                    u:object_r:sysfs_sensor_writable:s0
genfscon sysfs /devices/virtual/input/input4               u:object_r:sysfs_sensor_writable:s0
genfscon sysfs /devices/virtual/input/input6               u:object_r:sysfs_sensor_writable:s0
